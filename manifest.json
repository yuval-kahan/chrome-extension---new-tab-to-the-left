{
  "manifest_version": 3,
  "name": "Open Tab to the Left",
  "version": "1.0",
  "description": "Opens new tabs to the left of the current tab",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs"],
  "action": {
    "default_icon": {
      "16": "images/c.png",
      "48": "images/c.png",
      "128": "images/c.png"
    }
  }
}
